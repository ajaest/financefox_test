
<article class="post">
    <h1 class="title" >{{title}}</h1>
    <time datetime="{{created_at}}" class="created_at">{{created_at}}</datetime>
    <time datetime="{{updated_at}}" class="updated_at">{{updated_at}}</datetime>
    <img class="map" src="{{map_url}}"/>
    <img class="image" src="{{image_url}}"/>
    
    <section class="content">{{content}}</section>
</article>
